{
  "name" : "OpenTok Screen Sharing",
  "version" : "2.0",
  "manifest_version" : 2,
  "description" : "This Chrome extension is developed primary for http://tokbox.com/. It doesn't do anything except capture content of your screen.",
  "background": {
    "scripts": ["background-script.js"],
    "persistent": false
  },
  "externally_connectable": {
    "matches": [
      "https://*.xentricqa.com/*", "https://*.getzero.co/*", "http://localhost:8023/"
    ]
  },
  "key": "",
  "icons": {
    "16": "zero_logo.png",
    "48": "zero_logo.png",
    "128": "zero_logo.png",
    "256": "zero_logo.png"
  },
  "permissions": [ "desktopCapture" ],
  "web_accessible_resources": [
    "icon.png"
  ]
}
